{
  "name": "formol",
  "version": "1.0.5",
  "description": "An opiniated react form framework.",
  "main": "lib/formol.js",
  "repository": "https://github.com/Kozea/formol",
  "author": "Florian Mounier",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && webpack",
    "lint": "eslint src",
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook-debug": "node --inspect-brk ./node_modules/.bin/start-storybook -p 9001 -c .storybook",
    "storybook-deploy": "storybook-to-ghpages"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|sass)$": "<rootDir>/test/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ]
  },
  "pre-commit": "lint,test",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.49",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@dump247/storybook-state": "^1.5.0",
    "@storybook/addon-knobs": "^4.0.0-alpha.8",
    "@storybook/addon-options": "^4.0.0-alpha.8",
    "@storybook/addon-storyshots": "^4.0.0-alpha.8",
    "@storybook/addon-storysource": "^4.0.0-alpha.8",
    "@storybook/addons": "^4.0.0-alpha.8",
    "@storybook/react": "^4.0.0-alpha.8",
    "@storybook/storybook-deployer": "^2.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-react-static-displayname": "^0.0.2",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "date-fns": "2.0.0-alpha.7",
    "draft-js": "^0.10.5",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "import-sort": "^4.2.0",
    "import-sort-cli": "^4.2.0",
    "import-sort-parser-babylon": "^4.2.0",
    "import-sort-style-module": "^4.2.0",
    "jest": "^23.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "pre-commit": "^1.2.2",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-dropzone": "^4.2.11",
    "react-icons": "^2.2.7",
    "react-password-strength": "^2.3.1",
    "react-select": "^1.2.1",
    "react-test-renderer": "^16.4.0",
    "sass-loader": "^7.0.1",
    "webpack": "^4.10.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.4"
  },
  "files": [
    "lib/"
  ],
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "dependencies": {
    "bemboo": "^1.3.3",
    "deep-equal": "^1.0.1",
    "draftjs-to-html": "^0.8.4",
    "html-to-draftjs": "^1.4.0",
    "prop-types": "^15.6.1",
    "react-day-picker": "^7.1.9",
    "react-draft-wysiwyg": "^1.12.13",
    "regenerator-runtime": "^0.11.1"
  },
  "peerDependencies": {
    "date-fns": "^2.0.0-alpha.7",
    "react": "^16.x.x",
    "react-dom": "^16.x.x",
    "react-icons": "^2.2.7"
  }
}
