{
  "name": "formol",
  "version": "2.7.0",
  "description": "An opiniated react form framework.",
  "main": "lib/formol.js",
  "module": "src/index.js",
  "repository": "https://github.com/Kozea/formol",
  "author": "Florian Mounier",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && webpack",
    "lint": "eslint src stories storybook-addon-theme/src/* test",
    "fix": "prettier --write **/*.{js,jsx}",
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 9001",
    "storybook-debug": "node --inspect-brk ./node_modules/.bin/start-storybook -p 9001",
    "storybook-deploy": "storybook-to-ghpages"
  },
  "pre-commit": "lint,test",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-storyshots": "^5.1.11",
    "@storybook/addon-storysource": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-react-static-displayname": "^0.0.2",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "date-fns": "2.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.2.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-style-module": "^6.0.0",
    "jest": "^25.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "pre-commit": "^1.2.2",
    "quill-image-resize-module-react": "^3.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-syntax-highlighter": "^11.0.2",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.3.1",
    "scudd": "^0.0.1",
    "storybook4-state": "^1.5.3",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "module"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "bemboo": "^2.0.0",
    "deep-equal": "^1.0.1",
    "js-search": "^1.4.3",
