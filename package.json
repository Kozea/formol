    "react-window": "^1.8.5",
    "zxcvbn": "^4.4.2"
  },
  "peerDependencies": {
    "date-fns": "^2.0.0-alpha.25",
    "react": "^16.x.x",
    "react-dom": "^16.x.x",
    "react-icons": "^3.x.x"
  }
